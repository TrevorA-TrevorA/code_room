<%= flash.now[:errors] %>

<%= form_with(url: users_url, id: "create_account") do %>
<%= label(:user,:username, "Username:") %><br>
<%= text_field(:user, :username, autofocus: true) %><br>
<%= label(:user,:email, "Email:") %><br>
<%= email_field(:user,:email) %><br>
<%= label(:user,:password, "Password:") %><br>
<%= password_field(:user,:password) %><br>
<%= label(:user,:password_confirmation, "Confirm password:") %><br>
<%= password_field(:user,:password_confirmation) %><br>
<%= submit_tag("Create Account") %>
<% end %>